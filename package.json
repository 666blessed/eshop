{
  "name": "mern-ecommerce",
  "version": "1.0.0",
  "description": "MERN shopping app",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node backend/server",
    "server": "nodemon backend/server",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder",
    "data:destroy": "node backend/seeder -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/666blessed/eshop.git"
  },
  "author": "Kirill Katkov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/666blessed/eshop/issues"
  },
  "homepage": "https://github.com/666blessed/eshop#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "express-async-handler": "^1.2.0",
    "mongoose": "^6.2.1"
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
    "nodemon": "^2.0.15"
  }
}
